# mount point    fstype    device                   device2               flags
/boot            emmc      /dev/block/mmcblk0p5                           flags=backup=1
/recovery        emmc      /dev/block/mmcblk0p6
/efs             ext4      /dev/block/mmcblk0p17                          flags=backup=1;display="EFS"
/cache           ext4      /dev/block/mmcblk0p18                          flags=backup=1;wipeingui;wipeduringfactoryreset
/system          ext4      /dev/block/mmcblk0p19                          flags=backup=1;wipeingui
/preload         ext4      /dev/block/mmcblk0p20                          flags=backup=1;wipeingui;display="Preload"
/data            ext4      /dev/block/mmcblk0p21                          length=-16384 flags=backup=1;wipeingui
/external_sd     auto      /dev/block/mmcblk1p1     /dev/block/mmcblk1    flags=storage;removable
